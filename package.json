{
  "name": "books-api",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "author": "Jonathan Linat <jonathan.linat@gmail.com>",
  "version": "1.0.0",
  "license": "CC0",
  "scripts": {
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "seed": "export MONGO_COLLECTION_NAMES='[\"authors\"]' && node src/seeder/",
    "dev": "fastify start -w -l info -P src/",
    "start": "fastify start -l info src/",
    "test": "tap \"src/**/*.test.js\""
  },
  "dependencies": {
    "fastify": "3.27.4",
    "fastify-autoload": "3.11.0",
    "fastify-cli": "2.15.0",
    "fastify-cors": "6.0.3",
    "fastify-plugin": "3.0.1",
    "fastify-rate-limit": "5.8.0",
    "fastify-swagger": "5.1.0",
    "mongoose": "6.2.9"
  },
  "devDependencies": {
    "@faker-js/faker": "6.1.1",
    "dotenv": "16.0.0",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-prettier-standard": "4.0.1",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.2.0",
    "prettier": "2.6.1",
    "prettier-config-standard": "5.0.0",
    "tap": "16.0.1"
  }
}
